{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "todoName": {
      "type": "string",
      "minLength": 1
    },
    "dueDate": {
      "type": "string"
    },
    "refKey": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "done": {
      "type": "boolean"
    },
    "todoId": {
      "type": "string"
    },
    "attachmentUrl": {
      "type": "string"
    }
  },
  "required": [
    "todoName",
    "dueDate",
    "createdAt",
    "todoId",
    "done",
    "attachmentUrl"
  ],
  "additionalProperties": false
}